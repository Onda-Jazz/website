[
	{% for post in site.categories.shows %}
		{
		  "title"      : "{{ post.title | escape }}",
		  "venue"      : "{{ post.venue | escape }}",
		  "url"        : "{{ site.url }}{{ post.url }}",
		  "when"       : "{{ post.when }}",
		  "content": "{{ post.content | strip_html | strip_newlines | escape }}"
		} {% if forloop.last %}{% else %},{% endif %}
	{% endfor %}
]
